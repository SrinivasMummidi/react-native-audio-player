{
  "name": "native-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "rnef start",
    "lint": "eslint .",
    "test": "jest",
    "ios": "rnef run:ios",
    "android": "rnef run:android",
    "package:ios": "rnef package:ios --scheme NativeAppReact --configuration Release",
    "package:aar": "rnef package:aar --variant Release --module-name nativeappreact",
    "publish-local:aar": "pnpm package:aar && rnef publish-local:aar --module-name nativeappreact",
    "format": "prettier --ignore-path .gitignore --write ."
  },
  "dependencies": {
    "@callstack/react-native-brownfield": "^1.2.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-clipboard/clipboard": "^1.16.3",
    "fuse.js": "^7.1.0",
    "react": "19.1.0",
    "react-native": "0.80.1",
    "react-native-audio-recorder-player": "^3.6.14",
    "react-native-awesome-slider": "^2.9.0",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-nitro-modules": "^0.26.4",
    "react-native-reanimated": "^3.19.0",
    "react-native-svg": "^15.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@expo/fingerprint": "^0.11.6",
    "@react-native/babel-preset": "0.80.1",
    "@react-native/eslint-config": "0.80.1",
